<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>This will add the Face ID latch as seen on iPhone X to your lock screen. This is still considered beta and might cause layout issues.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>ml.festival.notchsimulator</string>
			<key>key</key>
			<string>latchEnabled</string>
			<key>label</key>
			<string>Enabled</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>default</key>
			<integer>0</integer>
			<key>defaults</key>
			<string>ml.festival.notchsimulator</string>
			<key>key</key>
			<string>latchOffsetY</string>
			<key>label</key>
			<string>Offset</string>
			<key>isNumeric</key>
			<false/>
		</dict>
	</array>
	<key>title</key>
	<string>Face ID Latch</string>
</dict>
</plist>
